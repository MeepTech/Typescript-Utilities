{
  "name": "@meep-tech/utils",
  "version": "0.0.5",
  "description": "Typescript Utilities",
  "main": "lib.js",
  "module": "lib.js",
  "types": "lib.d.ts",
  "homepage": "https://github.com/MeepTech/TypeScript-Utilities",
  "bugs": {
    "url": "https://github.com/MeepTech/TypeScript-Utilities/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeepTech/TypeScript-Utilities.git"
  },
  "license": "MIT",
  "author": {
    "name": "Meep.Tech",
    "email": "meepdottech@gmail.com",
    "url": "http://meep.tech"
  },
  "scripts": {
    "check-types": "tsc -noEmit -p \"tsconfig.tests.json\"",
    "test": "tsc -noEmit -p \"tsconfig.tests.json\" && jest",
    "build": "webpack",
    "build-dev": "webpack --env dev",
    "pack": "rm -r -f \"dist/npm/prod\" && mkdir -p \"dist/npm/prod\" && cd \"dist/npm/prod\" && npm pack \"../../../build/prod\"",
    "pack-dev": "rm -r -f \"dist/npm/dev\" && mkdir -p \"dist/npm/dev\" && cd \"dist/npm/dev\" && npm pack \"../../../build/dev\"",
    "clean-npm": "rm -r ./node_modules/ && rm package-lock.json && npm i"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.7",
    "@types/node": "18.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "file-system": "^2.2.2",
    "jest": "^28.1.3",
    "prettier": "^2.8.7",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "keywords": [
    "typescript",
    "utilities",
    "utils",
    "javascript",
    "protected",
    "ward",
    "warded",
    "warding",
    "lazy",
    "cached",
    "cache",
    "memoize",
    "memoized",
    "memoization",
    "decorators",
    "lazy-load",
    "lazyload",
    "lazyloading",
    "lazy-loaded",
    "lazy-loading",
    "lazyloaded",
    "lazyloading",
    "loop",
    "count",
    "size",
    "array",
    "iterable",
    "iterator",
    "js",
    "ts"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "version": "2022-03"
        }
      ]
    ]
  }
}
