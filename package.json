{
  "name": "@meep-tech/utils",
  "version": "0.0.2",
  "description": "Typescript Utilities",
  "main": "lib.js",
  "homepage": "https://github.com/MeepTech/TypeScript-Utilities",
  "bugs": {
    "url": "https://github.com/MeepTech/TypeScript-Utilities/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeepTech/TypeScript-Utilities.git"
  },
  "license": "MIT",
  "author": {
    "name": "Meep.Tech",
    "email": "meepdottech@gmail.com",
    "url": "http://meep.tech"
  },
  "scripts": {
    "check-types": "tsc -noEmit -p \"tsconfig.check.json\"",
    "test": "tsc -noEmit -p \"tsconfig.check.json\" && jest",
    "build-dev": "npm run check-types && webpack --env dev --progress && tsc -p \"tsconfig.dev.json\"",
    "build": "npm run check-types && webpack && tsc -p \"tsconfig.prod.json\"",
    "pack-dev": "npm run build-dev && npm run init-pack-dir-dev && npm run cd-pack-dir-dev && npm run pack-into-dir-dev",
    "pack": "npm run build && npm run init-pack-dir && npm run cd-pack-dir && npm run pack-into-dir",
    "init-pack-dir-dev": "rm -r -f \"dist/npm/dev\" && mkdir -p \"dist/npm/dev\"",
    "init-pack-dir": "rm -r -f \"dist/npm/prod\" && mkdir -p \"dist/npm/prod\"",
    "cd-pack-dir-dev": "cd \"dist/npm/dev\"",
    "cd-pack-dir": "cd \"dist/npm/prod\"",
    "pack-into-dir-dev": "cd \"dist/npm/dev\" && npm pack \"../../../build/dev\"",
    "pack-into-dir": "cd \"dist/npm/prod\" && npm pack \"../../../build/prod\""
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.7",
    "@types/node": "18.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "esbuild": "0.14.47",
    "jest": "^28.1.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0-dev.20230208",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "typescript",
    "utilities",
    "utils",
    "javascript",
    "protected",
    "ward",
    "warded",
    "warding",
    "lazy",
    "cached",
    "cache",
    "memoize",
    "memoized",
    "memoization",
    "decorators",
    "lazy-load",
    "lazyload",
    "lazyloading",
    "lazy-loaded",
    "lazy-loading",
    "lazyloaded",
    "lazyloading",
    "loop",
    "count",
    "size",
    "array",
    "iterable",
    "iterator",
    "js",
    "ts"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "version": "2022-03"
        }
      ]
    ]
  }
}